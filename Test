javascript:(function(){var u=prompt("Enter a link:");if(!u)return;if(!/^https?:\/\//i.test(u))u="https://"+u;var t=localStorage.getItem("tabTitle")||"Google Docs";var i=localStorage.getItem("tabIcon")||"https://ssl.gstatic.com/docs/documents/images/kix-favicon7.ico";var w=window.open("about:blank");if(!w){alert("Popups blocked");return;}w.document.open();w.document.write('<!DOCTYPE html><html><head><title>'+t+'</title><link rel="icon" href="'+i+'"><style>html,body{margin:0;width:100%;height:100%;overflow:hidden;background:#000}iframe{border:0;width:100%;height:100%}</style></head><body><iframe src="'+u+'" allowfullscreen referrerpolicy="no-referrer"></iframe></body></html>');w.document.close();})();